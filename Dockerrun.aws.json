{
  "AWSEBDockerrunVersion": "1",
  "Image": {
    "Name": "mwrona/boncer:0.0.1",
    "Update": "true"
  },
  "Ports": [
    {
      "HostPort": "80",
      "ContainerPort": "8080"
    }
  ],
  "Volumes": [
    {
      "HostDirectory": "/var/app/mydb",
      "ContainerDirectory": "/etc/mysql"
    }
  ],
  "Logging": "/var/log/user-management.log"
}